{
  "name": "registry",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "cp": "mkdir -p ../registry-server/src/main/resources/static/resource && cp -R ./dist/* ../registry-server/src/main/resources/static/resource",
    "start": "ng serve --env=dev --base-href /registry/ --disable-host-check",
    "build": "ng build --env=prod --prod && npm run cp",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "server:prod": "http-server dist -p 5000 -P http://localhost:8081 -c-1 --cors"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^4.4.6",
    "@angular/common": "^4.4.6",
    "@angular/compiler": "^4.4.6",
    "@angular/core": "^4.4.6",
    "@angular/forms": "^4.4.6",
    "@angular/http": "^4.4.6",
    "@angular/platform-browser": "^4.4.6",
    "@angular/platform-browser-dynamic": "^4.4.6",
    "@angular/router": "^4.4.6",
    "core-js": "^2.4.1",
    "file-saver": "^1.3.3",
    "jquery": "3.3.1",
    "lodash": "4.17.4",
    "ng2-cookies": "1.0.11",
    "ng2-translate": "^5.0.0",
    "ngx-logger": "^1.1.2",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.19",
    "angular2-moment": "^1.7.0",
    "air-datepicker": "^2.2.3",
    "toastr": "^2.1.4",
    "codemirror": "5.34.0"
  },
  "devDependencies": {
    "@angular/cli": "1.6.5",
    "@angular/compiler-cli": "^4.4.6",
    "@angular/language-service": "^4.4.6",
    "@compodoc/compodoc": "^1.0.5",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/jquery": "^3.3.0",
    "@types/lodash": "4.14.87",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
